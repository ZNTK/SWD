<Window x:Class="SWD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SWD"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
        </Grid.ColumnDefinitions>

        <Button x:Name="buttonImport" Content="Import" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="75" Grid.Row="0" Click="buttonImport_Click"/>
        <TextBox x:Name="textBoxImport" Grid.Row="0" HorizontalAlignment="Left" Height="23" Margin="100,-2,-429,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="407"/>
        <DataGrid AutoGenerateColumns="False" Grid.Row="1"
                  ItemsSource="{Binding}"
                  Name="mainDataGrid" />
        


    </Grid>
</Window>
